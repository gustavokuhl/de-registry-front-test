// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("REREGISTRY_PRISMA_URL") // uses connection pooling
  directUrl = env("REREGISTRY_URL_NON_POOLING") // uses a direct connection
}

model contrato {
  id      Int    @id @default(autoincrement())
  address String @unique
}
